{
	"name": "skapp",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"cycle": "git add . && git commit -m \"cycle\" && git push origin main && git checkout preview && git merge main && git push origin preview && git checkout main",
		"cycle2": "git checkout prod && git merge preview && git push origin prod && git checkout main",
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"pages:build": "npx @cloudflare/next-on-pages",
		"preview": "npm run pages:build && wrangler pages dev",
		"deploy": "npm run pages:build && wrangler pages deploy",
		"dev-windows": "next dev --turbopack",
		"build-windows": "next build",
		"preview-windows": "next build && npx serve .next",
		"deploy-win": "powershell -ExecutionPolicy Bypass -File deploy-windows.ps1",
		"deploy-win-bat": "deploy-windows.bat",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./env.d.ts"
	},
	"dependencies": {
		"framer-motion": "^12.23.12",
		"groq-sdk": "^0.30.0",
		"next": "15.4.6",
		"react": "19.1.0",
		"react-dom": "19.1.0"
	},
	"devDependencies": {
		"@cloudflare/next-on-pages": "^1.13.14",
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20.19.11",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"serve": "^14.2.4",
		"tailwindcss": "^4",
		"typescript": "^5",
		"vercel": "^46.0.2",
		"wrangler": "^4.32.0"
	}
}
